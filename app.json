{
  "expo": {
    "name": "rsc-spotify",
    "slug": "rsc-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "exai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bacon.rsc-spotify"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactServerFunctions": true
    }
  }
}
