{
  "expo": {
    "name": "rsc-spotify",
    "slug": "rsc-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "exspotify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bacon.rsc-spotify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "@bacons/apple-colors"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactServerFunctions": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "2ca4f70f-c242-45d1-b323-0fd2291b9c9c"
      }
    }
  }
}
